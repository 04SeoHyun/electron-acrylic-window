{
	"name": "electron-acrylic-window",
	"version": "0.5.5",
	"description": "Add vibrancy effect for electron",
	"main": "build_ts/index.js",
	"keywords": [
		"node",
		"vibrancy",
		"napi",
		"windows",
		"vibrant",
		"acrylic",
		"win10",
		"windows10",
		"blur",
		"fluent",
		"electron"
	],
	"scripts": {
		"build": "(node-gyp rebuild && npm run-script build-ts)",
		"build-ts": "npx tsc",
		"install": "node-gyp rebuild",
		"test": "npx tsc -p test && electron test/test.js",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"release": "standard-version"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint --cache --fix",
		"*.{js,jsx,ts,tsx,json,md}": "prettier --write"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Seo-Rii/electron-acrylic-window.git"
	},
	"author": "seorii <studioSeoRii@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Seo-Rii/electron-acrylic-window/issues",
		"email": "studioSeoRii@gmail.com"
	},
	"homepage": "https://github.com/Seo-Rii/electron-acrylic-window#readme",
	"gypfile": true,
	"dependencies": {
		"bindings": "^1.5.0",
		"node-addon-api": "^3.1.0",
		"node-gyp": "^7.1.2",
		"win32-displayconfig": "^0.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.0.0",
		"@commitlint/config-conventional": "^12.0.0",
		"@typescript-eslint/eslint-plugin": "^4.14.0",
		"@typescript-eslint/parser": "^4.14.0",
		"electron": "^11.1.1",
		"typescript": "^4.1.3",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"prettier": "^2.2.1",
		"husky": "=4",
		"pretty-quick": "^3.1.0",
		"standard-version": "^9.1.1",
		"lint-staged": "^10.5.4",
		"yarn": "^1.22.10"
	},
	"files": [
		"/LICENSE",
		"/README.md",
		"/tsconfig.json",
		"/package.json",
		"/logo.png",
		"/index.d.ts",
		"/binding.gyp",
		"src/**/*",
		"build_ts/**/*"
	]
}
